{
    "setup-worktree": ["rm -rf node_modules && ln -s $ROOT_WORKTREE_PATH/node_modules ./node_modules"],
    "setup-worktree-unix": ["rm -rf node_modules && ln -s $ROOT_WORKTREE_PATH/node_modules ./node_modules"],
